        <Label Content="Numero archivo" HorizontalAlignment="Left" Margin="10,66,0,0" VerticalAlignment="Top" Height="23" Width="107"/>
        <Button x:Name="BtnDiputado2019" Content="Diputado 2019" HorizontalAlignment="Left" Margin="289,229,0,0" VerticalAlignment="Top" Width="96" Click="BtnDiputado2019_Click"/>

    </Grid>
</Window>
